<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.3</LangVersion>
    <RootNamespace>FunctionZero.MvvmZero</RootNamespace>
    <AssemblyName>FunctionZero.MvvmZero</AssemblyName>
    <Version>2.0.3-pre-07</Version>
    <Copyright>FunctionZero Ltd. 2020</Copyright>
    <Description>A simple library for building cross-platform Xamarin Forms applications very quickly</Description>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/Keflon/FunctionZero.MvvmZero</PackageProjectUrl>
    <PackageTags>ICommand Command Mvvm Xamarin Forms iOS UWP Android</PackageTags>
    <PackageReleaseNotes>IHasOwnerPage events fixed. (OwnerPageAppearing, OwnerPageDisappearing)</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/Keflon/FunctionZero.MvvmZero</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FunctionZero.CommandZero" Version="1.0.2" />
    <PackageReference Include="Xamarin.Forms" Version="4.7.0.1080" />
  </ItemGroup>

  <ItemGroup>
    <None Include="License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
